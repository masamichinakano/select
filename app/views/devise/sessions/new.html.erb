<div class="top-session-image">
  <div class="top-session-wrap">
    <div class="player-header-wrap">
      <%= render "shared/session_header" %>
      <div class="player-header-contents">
        <div class="session-header-logo-warap">
          <h1><%= link_to "select", root_path, class:"player-header-logo" %></h1>
        </div>
      </div>
    </div>

    <%= render "shared/header" %>
    <%= form_with model: @user, url: new_user_session_path, class:"registration-session", local: true do |f| %>

      <div class="main-session-contents">
       <%= render "devise/shared/error_messages", model: f.object%>
        <div class="session-header-form-group">
          <div class='form-text-wrap'>
            <label class="session-header-form-text">ログイン画面</label>
          </div>
        </div>
        <div class="session-form-group">
          <div class='form-text-wrap'>
            <label class="session-form-text">email_address</label>
          </div>
          <%= f.email_field :email, placeholder:"select@com", class:"session-field" %>
        </div>
        <div class="session-form-group">
          <div class='form-text-wrap'>
            <label class="session-form-text">password</label>
          </div>
          <%= f.password_field :password, placeholder:"6文字以上の半角英数字", class:"session-field" %>
        </div>
        <div class='session-btn'>
          <%= f.submit "ログイン", class:"session-send" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
<%# <%= render "devise/shared/links" %>
