<div class="order-main-image">
  <div class="top-session-wrap">

    <%= render "shared/header" %>
    <div class="main-ofence-contents-wrap">
      <div class="ofence-contents-wrap">
        <div class="ofence-contents">
          <div class="ofence-data-wrap">
          </div>
        </div>

        <div class="ofence-contents">
          <div class="ofence-history-wrap">
            <% @matchs.each do |match| %>
              <div class="ofence-history">
              <%= link_to order_path(match.id) do %>
                <div class="match-name-wrap">
                  <p class="ofence-user-name"><%= current_user.name %></p>
                  <label class="ofence-form-text"> VS </label>
                  <p class="ofence-match-name"><%= match.match_name %></p>
                </div>
                <div class="match-date-wrap">
                  <p class="ofence-match-year"><%= match.year.name %>年</p>
                  <p class="ofence-match-month"><%= match.month.name %>月</p>
                  <p class="ofence-match-day"><%= match.day.name %>日</p>
                </div>
              <% end %>
              </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>


  </div>
</div>
<%= render "shared/footer" %>